# Example Environment Configuration
# Copy this to .env.local for personal overrides

# Development Mode (default)
# Leave empty for relative URLs + Vite proxy to backend on localhost:8000
VITE_API_BASE_URL=
VITE_WS_BASE_URL=

# Container Mode (when using make deploy-containers)
# Uncomment these for container testing with OAuth2-proxy
# VITE_API_BASE_URL=http://localhost:4180
# VITE_WS_BASE_URL=ws://localhost:4180

# Frontend Development Server
VITE_DEV_SERVER_HOST=localhost
VITE_DEV_SERVER_PORT=5173

# Environment
VITE_NODE_ENV=development
